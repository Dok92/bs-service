{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "checkJs": false,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "noEmit": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false
  }
}